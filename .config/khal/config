[calendars]

[[personal]]
path = ~/.calendars/framagenda/personal
type = calendar
color = dark blue

[[holidays]]
path = ~/.calendars/holidays
type = calendar
color = dark green

[[scikit-learn]]
path = ~/.calendars/scikit-learn
type = calendar
color = light green

[[probabl]]
path = ~/.calendars/probabl/loic@probabl.ai
type = calendar
color = brown

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = personal
highlight_event_days = True